  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Northwind</DataSourceName>
        <CommandText>SELECT tbl.TABLE_NAME,
	col.ORDINAL_POSITION,
	col.COLUMN_NAME,
	CASE WHEN col.DATA_TYPE LIKE '%CHAR%'
			THEN col.DATA_TYPE + '(' + CONVERT(VARCHAR, col.CHARACTER_MAXIMUM_LENGTH) + ')'
		ELSE col.DATA_TYPE
	END AS [DATA_TYPE]
FROM INFORMATION_SCHEMA.TABLES AS tbl
	INNER JOIN INFORMATION_SCHEMA.COLUMNS AS col ON col.TABLE_NAME = tbl.TABLE_NAME
WHERE tbl.TABLE_TYPE = 'BASE TABLE'
ORDER BY tbl.TABLE_NAME,
	col.ORDINAL_POSITION</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TABLE_NAME">
          <DataField>TABLE_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ORDINAL_POSITION">
          <DataField>ORDINAL_POSITION</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="COLUMN_NAME">
          <DataField>COLUMN_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DATA_TYPE">
          <DataField>DATA_TYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
